<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">
    <persistence-unit name="NewPersistenceUnit">
        <provider>org.apache.openjpa.persistence.PersistenceProviderImpl</provider>
        <class>com.mkyong.web.models.Abonent</class>
        <class>com.mkyong.web.models.City</class>
        <class>com.mkyong.web.models.Discount</class>
        <class>com.mkyong.web.models.Talking</class>
        <class>com.mkyong.web.models.Users</class>
        <class>com.mkyong.web.models.Tarif</class>
        <class>com.mkyong.web.models.Users</class>

        <properties>
            <property name="openjpa.RuntimeUnenhancedClasses" value="supported" />
            <property name="openjpa.QueryCache" value="false" />
            <property name="openjpa.DataCache" value="false" />
            <property name="openjpa.ConnectionURL" value="jdbc:postgresql://localhost:5433/phoneTalking"/>
            <property name="openjpa.ConnectionDriverName" value="org.postgresql.Driver"/>
            <property name="openjpa.ConnectionUserName" value="postgres"/>
            <property name="openjpa.ConnectionPassword" value="1"/>
            <property name="openjpa.Log" value="DefaultLevel=TRACE, Tool=INFO"/>
        </properties>
    </persistence-unit>
</persistence>
